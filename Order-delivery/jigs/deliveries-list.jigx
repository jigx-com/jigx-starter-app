title: Today's deliveries
type: jig.list

header:
  type: component.jig-header
  options:
    height: small
    children:
      type: component.image
      options:
        source:
          uri: https://img.freepik.com/premium-photo/redhead-delivery-man-blue-wall-with-happy-expression_1368-119486.jpg

data: =@ctx.datasources.all-deliveries
item:
  type: component.list-item
  options:
    title: =@ctx.current.item.dTrackingId
    subtitle: =@ctx.current.item.lStreetTo & ', ' & @ctx.current.item.lCountyTo
    # pridaj design pre uz completed deliveries
    style:
      isDisabled: =@ctx.current.item.dCategory = 'Completed' ? true:false
      isStrikeThrough: =@ctx.current.item.dCategory = 'Completed' ? true:false
    onPress: 
      type: action.go-to
      options:
        linkTo: delivery-detail
        parameters:
                  deliveryId: =@ctx.current.item.deliveryId
    rightElement: 
      element: value
      text: =@ctx.current.item.dCategory = 'Completed' ? 'Completed':$fromMillis($toMillis(@ctx.current.item.dDeliveryTime), '[h#1]:[m01][P]')
    leftElement: 
      element: avatar
      text: ""
      uri: data:image/png;base64,


